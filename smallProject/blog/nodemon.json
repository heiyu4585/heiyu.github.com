{
    "restartable": "rs",
    "ignore": [
    ".git",
    "node_modules/**/node_modules",
        "src"
],
    "verbose": true,
    "execMap": {
    "js": "node --harmony"
},
    "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
},
    "watch": [
    "test/fixtures/",
    "test/samples/",
        "blog/**"
],
    "env": {
    "NODE_ENV": "development"
},
    "ext": "js json"
}